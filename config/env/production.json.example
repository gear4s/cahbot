{
    "server":        "openirc.snt.utwente.nl",
    "nick":          "cah",
    "notifyUsers":   false,
    "startOnFirstJoin": true,
    "maxIdleRounds": 2,
    "timeLimit": 120,
    "timeBetweenRounds": 10,
    "timeWaitForPlayers": 180,
    "voicePlayers": false,
    "pointLimit":    10,
    "exitOnError": false,
    "stopOnLastPlayerLeave": false,
    "commandPrefixChars": ".!",
    "enableFastPick": true,
    "topic": {
        "position": "right",
        "separator": "::",
        "messages": {
            "on":  ["A game is running. Type %%join to get in on it!", "bold.yellow"],
            "off": "",
            "winner": ["reigning champion: <%= nick %>", "bold.yellow"]
        }
    },
    "connectCommands": [
        {
            "target": "",
            "message": ""
        }
    ],
    "joinCommands": {
        "#cah": [
            {
                "target": "",
                "message": ""
            }
        ]
    },
    "userJoinCommands": {
        "#cah": [
            {
                "target": "#cah",
                "message": "Hi <%= nick %>! Type %%join to play"
            }
        ]
    },
    "clientOptions": {
        "userName":             "cah",
        "channels":             ["#cah"],
        "messageSplit":         410,
        "floodProtection":      true,
        "floodProtectionDelay": 1000
    },
    "supported": {
        "modes": 4,
        "nicklength": 16
    },
    "beers": [ "Blue Moon", "Pabst Blue Ribbon", "Yuengling", "Stella Artois", 
               "Modelo", "Fat Tire Amber Ale", "Magic Hat", "Samuel Adams",
               "Sierra Nevada", "Leffe Blonde", "Duvel", "Warsteiner", "Erdinger Weiss"
    ]
}
